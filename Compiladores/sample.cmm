/* Sequencia de Fibonacci em Cmm - Recursivo */

prototipo real portugal(inteiro);

prototipo semretorno paralelepipedo(inteiro)
, preda(semparam);


semretorno principal(semparam) {

     inteiro i, n;

     imprima("Digite a qtd de termos da sequencia: ");
     leia(n);
     imprima("A sequencia de Fibonacci solicitada eh: ");
     imprima(enter);
     para (i=0; i<n; i=i+1) 
            imprima(fibonacci(i+1));
      retorne;
}

inteiro fibbonaci(inteiro num) {
      inteiro variavel1, variavel2;

      se ((num==1) || (num==2)) retorne 1;
      senao 
             retorne (fibonacci(num-1)+fibonacci(num-2));
}

booleano raiz(inteiro num) {
      inteiro variavel3, variavel4;
      se ((num==1) || (num==2)) retorne 1;
      senao 
             retorne (fibonacci(num-1)+fibonacci(num-2));
}



